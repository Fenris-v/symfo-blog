<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="fenris.theme_provider" class="Fenris\ThemeBundle\ThemeProvider" public="true">
            <argument type="service" id="fenris.theme"/>
        </service>

        <service id="fenris.some_theme_provider" class="Fenris\ThemeBundle\Providers\SomeThemeProvider" public="false"/>
        <service id="fenris.theme" alias="fenris.some_theme_provider" public="false"/>

        <service id="Fenris\ThemeBundle\ThemeProvider" alias="fenris.theme_provider" public="false"/>
    </services>
</container>
